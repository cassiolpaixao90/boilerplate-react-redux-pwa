{
  "presets": [ "es2015", "stage-2",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        },
        "debug": false
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "universal-import"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
