{
  "name": "boilerplate-react-redux-pwa",
  "version": "1.0.0",
  "description": "It's sample boilerplate with pwa + react + redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/cassiolpaixao90/boilerplate-react-redux-pwa.git"
  },
  "engines": {
    "npm": ">=8"
  },
  "author": "Cássio Paixão",
  "license": "MIT",
  "keywords": [
    "React",
    "React-Router",
    "Redux",
    "Redux-Saga",
    "Webpack",
    "ES6",
    "PWA"
  ],
  "private": true,
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "babel-polyfill": "6.8.0",
    "lodash": "latest",
    "cross-env": "^5.0.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "cross-env ENABLE_TUNNEL=true babel-node bin/server.js",
    "lint": "node_modules/.bin/esw ./webpack/webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "cross-env NODE_ENV=production babel-node",
    "postbuild": "babel-node bin/server.js"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "ngrok": "2.2.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "8.5.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  }
}
